
lista=[]
maior = 0
menor =999999999
soma=0

for i in range(0,12):
        num= int(input(f'Digite o valor do item  {i+1} da lista: '))
        lista.append(num)
        soma = soma+num

        while lista[i]>maior:
            maior = lista[i]
        while lista[i]<menor:
            menor = lista[i]



print(soma)
soma = soma - menor - maior


print(maior)
print(menor)
print(lista)
print(soma)

